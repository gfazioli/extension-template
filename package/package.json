{
  "name": "mantine-extension-template",
  "description": "A template for Mantine extensions",
  "packageManager": "yarn@4.0.1",
  "version": "0.1.1",
  "workspaces": [
    "docs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtivital/mantine-extension-template.git"
  },
  "keywords": [
    "mantine",
    "extension"
  ],
  "author": "Vitaly Rtishchev <rtivital@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rtivital/mantine-extension-template/issues"
  },
  "homepage": "https://github.com/rtivital/mantine-extension-template#readme",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "module": "./dist/esm/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.mts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./styles.css": "./dist/styles.css",
    "./styles.layer.css": "./dist/styles.layer.css"
  },
  "peerDependencies": {
    "@mantine/core": ">=7.0.0",
    "@mantine/hooks": ">=7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
