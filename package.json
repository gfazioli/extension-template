{
  "name": "mantine-extension-template",
  "version": "0.0.7",
  "description": "Template for Mantine extensions",
  "types": "./declarations/index.d.ts",
  "main": "./dist/index.js",
  "exports": {
    "types": "./declarations/index.d.ts",
    "default": "./dist/index.js"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "prettier:test": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "build:dts": "tsc && rimraf temp",
    "build": "rollup --config .config/rollup.config.js",
    "release": "npm run build && esno scripts/release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtivital/mantine-extension-template.git"
  },
  "keywords": [
    "mantine",
    "extension"
  ],
  "author": "Vitaly Rtishchev <rtivital@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rtivital/mantine-extension-template/issues"
  },
  "homepage": "https://github.com/rtivital/mantine-extension-template#readme",
  "peerDependencies": {
    "@emotion/react": ">=11.9.0",
    "@mantine/core": ">=5.0.0-alpha.22",
    "@mantine/hooks": ">=5.0.0-alpha.22"
  },
  "dependencies": {},
  "devDependencies": {
    "@emotion/react": "^11.9.3",
    "@mantine/core": "^5.0.0-alpha.22",
    "@mantine/hooks": "^5.0.0-alpha.22",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.0.6",
    "@types/react": "^18.0.15",
    "@types/signale": "^1.4.4",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "esbuild": "^0.14.49",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-mantine": "2.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "esno": "^0.16.3",
    "execa": "^6.1.0",
    "fs-extra": "^10.1.0",
    "new-github-release-url": "^2.0.0",
    "open": "^8.4.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.0",
    "rollup-plugin-esbuild": "^4.2.3",
    "rollup-plugin-node-externals": "^2.2.0",
    "signale": "^1.4.0",
    "simple-git": "^3.10.0",
    "typescript": "^4.7.4",
    "yargs": "^17.5.1"
  }
}
