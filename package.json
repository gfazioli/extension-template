{
  "name": "mantine-extension-template",
  "description": "A template for Mantine extensions",
  "packageManager": "yarn@4.0.1",
  "version": "0.10.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtivital/mantine-extension-template.git"
  },
  "keywords": [
    "mantine",
    "extension"
  ],
  "author": "Vitaly Rtishchev <rtivital@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rtivital/mantine-extension-template/issues"
  },
  "homepage": "https://github.com/rtivital/mantine-extension-template#readme",
  "scripts": {
    "clean": "rimraf dist",
    "generate-dts": "esno scripts/generate-dts",
    "build": "rollup -c rollup.config.mjs && npm run generate-dts",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache --ext .ts,.tsx .",
    "jest": "jest",
    "test": "npm run prettier:check && npm run typecheck && npm run lint && npm run jest",
    "release": "npm test && esno scripts/release"
  },
  "peerDependencies": {
    "@mantine/core": ">=7.0.0",
    "@mantine/hooks": ">=7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@mantine/core": "^7.1.7",
    "@mantine/hooks": "^7.1.7",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/fs-extra": "^11",
    "@types/identity-obj-proxy": "^3",
    "@types/jest": "^29.5.6",
    "@types/node": "^20.8.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/signale": "^1",
    "@types/yargs": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-mantine": "^3.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "new-github-release-url": "^2.0.0",
    "open": "^9.1.0",
    "postcss": "^8.4.31",
    "postcss-preset-mantine": "^1.9.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "signale": "^1.4.0",
    "simple-git": "^3.20.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "version-next": "^1.0.2",
    "yargs": "^17.7.2"
  }
}
